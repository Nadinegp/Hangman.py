from tkinter import *

# Create an instance of window
win = Tk()

# Set the geometry of the window
win.geometry("600x600")

C1 = Canvas(win, width=600, height=600)

# Coordinates of the line +25 at x1
coordinates = 75,250,30,250

# Draw a dashed vertical line, 5px dash and 1px space
C1.create_line(coordinates, dash=(31,30))
C1.pack()

win.mainloop()